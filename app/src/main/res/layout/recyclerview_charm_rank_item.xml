<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/tv_onrank"
        android:layout_width="wrap_content"
        android:layout_height="14dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="14dp"
        android:text="未上榜"
        android:textColor="#B3FFFFFF"
        android:textSize="10sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_rank_no_bg"
        android:layout_width="22dp"
        android:layout_height="16dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="2dp"
        android:text="1"
        android:gravity="center_horizontal"
        android:textSize="12sp"
        android:textColor="#EB9200"
        android:background="@drawable/rank_no_bg"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_onrank" />

    <ImageView
        android:id="@+id/iv_avatar"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="10dp"
        app:layout_constraintLeft_toRightOf="@+id/tv_onrank"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="11dp"
        android:text="124565231356"
        android:textColor="#3E3E3E"
        android:textSize="15sp"
        app:layout_constraintLeft_toRightOf="@id/iv_avatar"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/iv_gender"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:layout_marginLeft="11.6dp"
        android:layout_marginTop="3.6dp"
        app:layout_constraintLeft_toRightOf="@id/iv_avatar"
        app:layout_constraintTop_toBottomOf="@id/tv_username" />

    <ImageView
        android:id="@+id/iv_icon1"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:layout_marginLeft="3dp"
        android:layout_marginTop="3.6dp"
        app:layout_constraintLeft_toRightOf="@id/iv_gender"
        app:layout_constraintTop_toBottomOf="@id/tv_username" />

    <ImageView
        android:id="@+id/iv_icon2"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:layout_marginLeft="3dp"
        android:layout_marginTop="3.6dp"
        app:layout_constraintLeft_toRightOf="@id/iv_icon1"
        app:layout_constraintTop_toBottomOf="@id/tv_username" />

    <ImageView
        android:id="@+id/iv_icon3"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:layout_marginLeft="3dp"
        android:layout_marginTop="3.6dp"
        app:layout_constraintLeft_toRightOf="@id/iv_icon2"
        app:layout_constraintTop_toBottomOf="@id/tv_username" />

    <TextView
        android:id="@+id/tv_rich_value"
        android:layout_width="wrap_content"
        android:layout_height="14dp"
        android:layout_marginLeft="214.5dp"
        android:layout_marginTop="14dp"
        android:text="魅力值"
        android:textColor="#999999"
        android:textSize="10sp"
        app:layout_constraintLeft_toRightOf="@id/iv_avatar"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_rich_day_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="201dp"
        android:text="45527891"
        android:textColor="#FC4593"
        android:textSize="14sp"
        app:layout_constraintLeft_toRightOf="@id/iv_avatar"
        app:layout_constraintTop_toBottomOf="@id/tv_rich_value"
        />

</androidx.constraintlayout.widget.ConstraintLayout>